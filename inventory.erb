[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.5.0
openshift_image_tag=v1.5.0
openshift_public_hostname=legerity.schiznik.net
openshift_master_default_subdomain=puggs.net
openshift_hosted_metrics_deploy=true

openshift_master_overwrite_named_certificates=true
openshift_master_named_certificates=[{"certfile": "/etc/letsencrypt/live/legerity.schiznik.net/cert.pem", "keyfile": "/etc/letsencrypt/live/legerity.schiznik.net/privkey.pem", "cafile": "/etc/letsencrypt/live/legerity.schiznik.net/fullchain.pem"}]


[masters]
legerity.schiznik.net openshift_schedulable=true

[nodes]
legerity.schiznik.net openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
